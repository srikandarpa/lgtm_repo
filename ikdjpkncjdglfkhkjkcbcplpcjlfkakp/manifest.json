{
  "author": "Paris Tjunski",
  "name": "Cookie Deleter",
  "short_name": "CookieDel",
  "homepage_url": "https://cookiedeleter.com",
  "description": "browser cookie deleter",
  "version": "1.0",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "logo-128.png",
    "default_title": "Cookie Deleter"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "128": "logo-128.png"
  },
  "permissions": [
    "browsingData",
    "tabs",
    "storage",
    "webRequest",
    "webRequestBlocking",
    "http://*/*",
    "https://*/*"
  ],
  "update_url": "https://edge.microsoft.com/extensionwebstorebase/v1/crx"
}